# This file is managed by puppet
__version__ = 19
# News server definitions
[servers]
<% @servers.each_pair do |key, value_hash| -%>
[[<%= key %>]]
name = <%= key %>
host = <%= value_hash['server_url'] %>
enable = <%= value_hash['enabled'] || '1' %> 
username = "<%= value_hash['username'] || '' %>"
password = "<%= value_hash['password'] || '' %>"
connections = <%= value_hash['connections'] %>
ssl = <%= value_hash['ssl'] || '0' %>
port = <%= value_hash['port'] || '119' %>
timeout = <%= value_hash['timeout'] || '120' %>
fillserver = <%= value_hash['fillserver'] || '0' %>
optional = <%= value_hash['optional'] || '0' %>
retention = <%= value_hash['retention'] || '0' %>
<% end -%>
# Category definitions
[categories]
[[*]]
priority = 0
pp = 3
name = *
script = None
newzbin = ""
dir = ""
<% @categories.each_pair do |key, value_hash| -%>
[[<%= key %>]]
priority = <%= value_hash['priority'] || '0' %>
pp = <%= value_hash['postprocessing'] || '3' %>
name = <%= key %>
script = None
newzbin = ""
dir = <%= value_hash['directory'] %>
<% end -%>
